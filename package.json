{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A minimal opinionated react boilerplate.",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"**/*.js\"",
    "precommit": "lint-staged",
    "start": "webpack-dev-server --config ./webpack.config.babel.js --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atonem/react-boilerplate.git"
  },
  "keywords": [
    "react",
    "boilerplate"
  ],
  "author": "Anton Emmerik <anton@emmerik.se>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/atonem/react-boilerplate/issues"
  },
  "homepage": "https://github.com/atonem/react-boilerplate#readme",
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.{js,css}": [
      "prettier-eslint --list-different"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.4.5",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.4.0",
    "prettier": "^1.15.2",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "react-hot-loader": "^4.3.12",
    "style-loader": "^0.23.1",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "autoprefixer": "^9.3.1",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  }
}
