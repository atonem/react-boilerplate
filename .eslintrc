{
  "root": true,
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "standard",
    "prettier",
    "prettier/standard"
  ],
  "plugins": [
    "import",
    "prettier",
    "standard"
  ],
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "settings": {
    "import/resolver": {
      "babel-module": { }
    },
    "react": {
      "version": "16.6"
    }
  },
  "rules": {
    "space-before-function-paren": 0,
    "new-cap": 0,
    "prettier/prettier": 2,
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0
  }
}

